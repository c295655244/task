<script type="text/javascript">
$(function () {
	var activeIndex=0;
	$('.nav-map').parent().bind('mouseover',function(){
		$('#articleImgLink,#articleHeaderLink,#articleDescLink').prop('href',$(this).find('.nav-map').data('articleurl'));
		var $currentPath=$(this).find('.nav-map');
		$('#currentMap').fadeOut('fast',function(){
			$(this).prop('src',$currentPath.prop('src')).prop('alt',$currentPath.prop('alt')).fadeIn('fast');
			});
		$('#articleHeaderLink').text($currentPath.prop('alt'));
		$('#articleDescLink').text($currentPath.data('articleabstract'));
		$('.nav-map-wrapper > li').removeClass('active').find('.float-layer').css({'opacity':'0.3'});
		$(this).parent().addClass('active').find('.float-layer').css({'opacity':'0'});
		
		})
	
	function showImage(){
		var imageLength=$('.nav-map').length;
		if ((activeIndex+1)<imageLength)
			activeIndex++;
		else
			activeIndex=0;
		$('.nav-map').eq(activeIndex).mouseover();
		}
	var rollImage=setInterval(showImage,5000);
})
</script>
